<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns:fx="http://javafx.com/fxml" xmlns="http://javafx.com/javafx"
            fx:controller="org.roiugit.NewProofController">
    <VBox fx:id="contentVBox" spacing="10" alignment="CENTER">
        <TextField fx:id="premiseField" promptText="Enter a premise"/>
        <Button fx:id="addPremiseButton" text="Add Premise" onAction="#addPremise"/>
        <Button fx:id="createProofButton" text="Create Proof" onAction="#createProof"/>
    </VBox>
</AnchorPane>
